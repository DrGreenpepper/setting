struct edge{
	int next,to;
}e[maxm*2];
int head[maxn],tot=0;
//please init head[];
void add(int u,int v){
	edge[tot].to=v;edge[tot].next=head[u];head[u]=tot++;
	edge[tot].to=u;edge[tot].next=head[v];head[v]=tot++;
}

